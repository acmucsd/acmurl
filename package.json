{
  "name": "acmurl",
  "version": "1.0.0",
  "description": "ACM UC San Diego's URL Shortener",
  "main": "index.js",
  "scripts": {
    "start": "cd server && yarn start",
    "start-dev": "cd client && node_modules/webpack/bin/webpack.js -p --config ./webpack.config.js ; cd ../server && yarn start"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/acmucsd/acmurl.git"
  },
  "author": "acmucsd",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/acmucsd/acmurl/issues"
  },
  "homepage": "https://github.com/acmucsd/acmurl#readme",
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-google": "^0.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "husky": "^3.0.1",
    "lint-staged": "^9.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "./node_modules/.bin/eslint --fix",
