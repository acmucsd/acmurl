  "dependencies": {
    "body-parser": "^1.19.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-config-airbnb-bundle": "^3.0.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.6.9"
  }
